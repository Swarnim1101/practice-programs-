
#include <stdio.h>
 struct employee{
    char    name[30];
    int     age;
    float    pn;
    float   salary;
};
 
int main()
{
   
    struct employee emp;
    
    printf("\nEnter details :\n");
    printf("Name :");         
    gets(emp.name);
    printf("Age :");            
    scanf("%d",&emp.age);
    printf("Phone Number :");            
    scanf("%f",&emp.pn);
    printf("Salary :");        
    scanf("%f",&emp.salary);
   
    printf("\nEntered detail is:");
    printf("Name: %s\n",emp.name);
    printf("Age: %d\n",emp.age);
    printf("Phone Number: %f\n",emp.pn);
    printf("Salary: %f\n",emp.salary);
    return 0;
}
